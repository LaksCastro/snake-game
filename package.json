{
  "name": "snake-game",
  "version": "1.0.0",
  "description": "A implementation of classic Snake Game with HTML5 Canvas Engine. Written in Typescript: available in https://lakscastro.github.io/snake-game/",
  "main": "index.js",
  "scripts": {
    "dev": "parcel public/index.html --out-dir development -p 3000",
    "build": "parcel build public/index.html --out-dir build --public-url /snake-game",
    "deploy": "node ./publish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LaksCastro/snake-game.git"
  },
  "keywords": [
    "parcel",
    "snake",
    "game",
    "web",
    "responsive",
    "canvas",
    "javascript"
  ],
  "author": "Laks Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LaksCastro/snake-game/issues"
  },
  "homepage": "https://lakscastro.github.com/snake-game",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "gh-pages": "^3.0.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-pwa-manifest": "^3.0.6",
    "sass": "^1.26.3",
    "typescript": "^3.9.5",
    "workbox-cli": "^5.1.3"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "lodash.debounce": "^4.0.8"
  },
  "pwaManifest": {
    "name": "Snake Game",
    "shortName": "Snake",
    "startURL": "/snake-game/",
    "theme": "#7AAB5E",
    "generateIconOptions": {
      "baseIcon": "./public/favicon.svg",
      "sizes": [
        32,
        64,
        192,
        384,
        512,
        780
      ],
      "genFavicons": true
    }
  }
}
